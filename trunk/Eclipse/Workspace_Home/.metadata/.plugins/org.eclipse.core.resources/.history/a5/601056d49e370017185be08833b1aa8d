package ch.mitti.listen;

public class DVNode {
	private Element item = null;
	private Node next = null;
	private Node prev = null;
	
	/**
	 * Default Konstruktor
	 */
	public DVNode()
	{
		item = null;
		next = null;
	}
	
	public DVNode(Element x)
	{
		item = x;
		next = null;
	}
	
	public Node getNext()
	{
		return this.next;
	}
	
	public Node getPrev(){
		return this.prev;
	}
	
	public void setNext(Node newNext)
	{
		this.next = newNext;
	}
	
	public void setPrev(Note newPrev){
		this.prev = newPrev;
	}
	
	public Element getItem()
	{
		return this.item;
	}
	
	public void setItem(Element newItem)
	{
		this.item = newItem;
	}
	public void show()
	{
		item.show();
	}
}
