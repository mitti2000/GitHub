public class ParkingGarage {
	private int places; // Anzahl freie Plätze
	
	public ParkingGarage ( int places)
	{
		if (places < 0)
		{
			throw new IllegalArgumentException("Parameter < 0");
		}
		
		this.places = places;
	}
	
	public  void enter()
	{
		while (isFull());
		decrementPlacec();
	}
	
	public synchronized void leave()
	{
		places++;
	}
	

}
