package ch.mitti.fibonacci;

public class Fibonacci {
	
	int res;

	public int rechnen(int anzahl){
		if(anzahl-1!=1 && anzahl-2!=0){
			res = rechnen(anzahl-1) + rechnen(anzahl-2);
		}
		
		else res = anzahl-1 + anzahl-2;
		return res;
	}
	
	
	public static void main(String[] args){
		Fibonacci fibonacci = new Fibonacci();
		int res = fibonacci.rechnen(3);
		System.out.println(res);
	}
}
