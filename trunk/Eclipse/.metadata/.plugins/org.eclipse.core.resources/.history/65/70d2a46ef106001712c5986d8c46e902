package controller;

public class Test {
	
	
	public boolean equals(Object obj) { //Spezieller Objekttyp
	 	if (this == obj) return true;  //Wenn Referenz gleich, dass ist es das gleich Objekt
	 	if (obj == null) return false; //Wenn null übergeben kann nicht verglichen werden
	 	if (getClass() != obj.getClass()) return false; //ungleicher Objekttyp
	 	Person other = (Person) obj;  //Objekt wird in Person gecastet
	 	if (email == null) {  //Hat E-Mail einen gültigen Wert?
	 	 	if (other.email != null) return false; 
	 	 	} 
	 	else if (!email.equals(other.email)) return false; //String wird mit Equals verglichen
	 	return true; 	
	 	}

	
	
	
}
