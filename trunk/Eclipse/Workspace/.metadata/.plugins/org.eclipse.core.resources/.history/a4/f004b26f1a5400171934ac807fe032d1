package ch.mitdom.compresstutorial;

import java.awt.event.WindowAdapter;
import java.awt.event.WindowEvent;
import java.awt.event.WindowListener;

/**
 * Der GUI Listener dient dazu, dass beim Schliessen eines Fensters wieder zum
 * MainGui zur√ºckgekehrt werden kann.
 */
public class GuiListener extends WindowAdapter{
	private ActionGui gui;
	public GuiListener(ActionGui gui){
		this.gui = gui;
	}
	public void windowClosing(WindowEvent e){
		new MainGui();
	}
}