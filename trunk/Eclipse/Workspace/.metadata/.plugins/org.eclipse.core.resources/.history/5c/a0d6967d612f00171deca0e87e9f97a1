package ch.mitti.quadrate;
import ch.aplu.turtle.*;

public class Quadrate {
	
	Turtle t;
	
	public Quadrate(int max, int min){
		t = new Turtle();
		t.speed(200);
		t.hideTurtle();
		t.setX(200);
		t.setY(200);
		t.right(90);
		
		zeichnen(max, min);
	}
	
	public void zeichnen(int max, int min){
		t.forward(50);
	}

	public static void main(String[] args) {
		Quadrate quadrate = new Quadrate(50, 2);

	}

}
