package ch.mitti.yahtzee.main;

import javax.swing.JFrame;

import ch.mitti.yahtzee.view.DiceBoardView;
import ch.mitti.yahtzee.view.GameBoardView;

public class Main {
	static DiceBoardView diceBoard;
	public static void main(String[] args){
		//GameBoardView game = new GameBoardView();
		
		diceBoard = new DiceBoardView();
		JFrame frame = new JFrame();
		frame.setSize(800, 800);
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		frame.add(diceBoard);
		frame.setVisible(true);
	}
}
