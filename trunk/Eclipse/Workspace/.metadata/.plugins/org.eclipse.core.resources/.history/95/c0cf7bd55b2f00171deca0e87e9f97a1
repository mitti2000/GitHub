package ch.mitti.turtle;
import ch.aplu.*;
import ch.aplu.turtle.Turtle;

public class Kochkurve {
	Turtle t;
	public Kochkurve(int min){
		t = new Turtle();
		t.speed(200);
		t.hideTurtle();
		t.setX(-200);
		t.setY(-50);
		t.right(90);
	}
	
	public void zeichne(int min){
		t.forward(20);
	}

	public static void main(String[] args) {
		Kochkurve kochkurve = new Kochkurve(5);
	}

}
