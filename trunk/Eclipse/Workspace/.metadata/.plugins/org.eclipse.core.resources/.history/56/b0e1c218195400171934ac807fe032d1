package ch.mitdom.compresstutorial;

public class DecompressorFactory {
	IDecompressor decompressor;
	
	public IDecompressor getCompressor(String fileType, DecompressorGui decompressorGui){
		switch(fileType){
			case(".zip"):
				decompressor =  new DecompressorZip();
				break;
				
			case(".7z"):
				decompressor = new Decompressor7z();
				break;
			
			default:
				decompressor = null;
				break;
		}
		return decompressor;
	}
}
