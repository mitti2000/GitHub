package ch.mitdom.compresstutorial;

import java.io.File;

/**
 * Interface für die Dekompression dient hauptsächlich der Polymorphie
 */
public interface IDecompressor {
	
	/**
	 * Führt die Dekompression aus
	 * @param decompressorGui Dekompressor GUI vom Typ decompressorGui
	 * @return
	 */
	public boolean decompress(DecompressorGui decompressorGui);
}
