package ch.mitti.turtle;
import ch.aplu.*;
import ch.aplu.turtle.Turtle;

public class Kochkurve {
	Turtle t;
	public Kochkurve(){
		t = new Turtle();
		t.hideTurtle();
		t.setX(-200);
		t.setY(0);
	}
	
	public void zeichne(int min){
		t.forward(20);
	}

	public static void main(String[] args) {
		Kochkurve kochkurve = new Kochkurve();
		kochkurve.zeichne(5);

	}

}
